   "source": [
    "from ete3 import NCBITaxa\n",
    "\n",
    "ncbi = NCBITaxa()\n",
    "\n",
    "# Fetching tree for Corynebacterium glutamicum\n",
    "tree = ncbi.get_lineage(1718)\n",
    "names = ncbi.get_taxid_translator(tree)\n",
    "for taxid in tree:\n",
    "    print(f\"{taxid} {names[taxid]}\")\n"
   ],
